<%- include ("../partials/header")%>

<form action="/account/addMember" method="post">
    <input type="text" name="id" value="<%= account._id%>" hidden>
    <label for="membername">Enter Member Name:</label>
    <input type="text" name="mName" required>
    <label for="memberemail">Enter Member email:</label>
    <input type="email" name="mEmail" required>
    <input type="submit" value="submit" name="submit" >
</form>

